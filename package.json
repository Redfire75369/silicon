		"@sveltejs/enhanced-img": "0.3.8",
		"@sveltejs/kit": "2.5.28",
		"@sveltejs/vite-plugin-svelte": "4.0.0-next.7",
		"@types/node": "22.5.5",
		"drawio-export-puppeteer": "0.2.0",
		"mdsvex": "0.12.3",
		"mdsvex-relative-images": "1.0.3",
